                    android:enabled="true" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_teleop_cargo_top"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseCargoTopInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_teleop_cargo_top"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseCargoTopInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_cargo_top_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/cargoTop"
                app:errorEnabled="true"
                tools:ignore="UnknownId">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/teleop_cargo_top_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:enabled="true" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_teleop_cargo_middle"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseCargoMiddleInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_teleop_cargo_middle"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseCargoMiddleInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_cargo_middle_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/cargoMiddle"
                app:errorEnabled="true"
                tools:ignore="UnknownId">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/teleop_cargo_middle_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:enabled="true" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_teleop_cargo_bottom"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseCargoBottomInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_teleop_cargo_bottom"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseCargoBottomInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_cargo_bottom_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/cargoBottom"
                app:errorEnabled="true"
                tools:ignore="UnknownId">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/teleop_cargo_bottom_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:enabled="true" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>




        <Button
            android:id="@+id/save_btn"
            android:layout_width="match_parent"
            android:layout_height="30pt"
            android:onClick="saveData"
            android:text="@string/saveButton"/>

    </LinearLayout>

