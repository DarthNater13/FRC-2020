<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ScrollView01"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    android:scrollbars="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="12pt"
            android:text="@string/cargoShipTitle"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_hatchPanelsOnCargoShip"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseCargoShipHatchPanelInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_hatchPanelsOnCargoShip"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseCargoShipHatchPanelInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_cargo_ship_hatch_panel_input_layout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:hint="@string/hatchPanelsOnCargoShip"
                app:errorEnabled="true"
                tools:ignore="UnknownId">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/teleop_cargo_ship_hatch_panel_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:enabled="true"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:inputType="number" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_teleop_cargo_in_cargo_ship"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseCargoInCargoShipInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_teleop_cargo_in_cargo_ship"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseCargoInCargoShipInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_cargo_in_cargo_ship_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/cargoInCargoShip"
