                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_hatch_panel_middle_input_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/hatchPanelMiddle"
                app:errorEnabled="true"
                tools:ignore="UnknownId">

                <android.support.design.widget.TextInputEditText
                    android:id="@+id/teleop_hatch_panel_middle_input"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="number"
                    android:clickable="false"
                    android:cursorVisible="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:enabled="true" />
            </android.support.design.widget.TextInputLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/decrease_teleop_hatch_panel_bottom"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="decreaseHatchPanelBottomInput"
                android:text="@string/minus"/>
            <Button
                android:id="@+id/increase_teleop_hatch_panel_bottom"
                android:layout_width="30pt"
                android:layout_height="30pt"
                android:onClick="increaseHatchPanelBottomInput"
                android:text="@string/plus"
                />

            <android.support.design.widget.TextInputLayout
                android:id="@+id/teleop_hatch_panel_bottom_input_layout"
